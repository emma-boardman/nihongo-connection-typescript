{
  "Main": {
    "uid": {
      "type": "UID",
      "config": {
        "label": "Class uid"
      }
    },
    "class_title": {
      "type": "StructuredText",
      "config": {
        "single": "heading1, heading2, heading3, heading4, heading5, heading6",
        "label": "Class Title"
      }
    },
    "class_1_bullet_preview": {
      "type": "StructuredText",
      "config": {
        "single": "paragraph",
        "label": "Class 1 Bullet Preview"
      }
    },
    "class_type": {
      "type": "Select",
      "config": {
        "options": [
          "Course",
          "Bootcamp",
          "By Application",
          "Membership",
          "Friday Conversation Club"
        ],
        "label": "Class type"
      }
    },
    "class_location": {
      "type": "Select",
      "config": {
        "options": [
          "Online",
          "In Edinburgh"
        ],
        "default_value": "Online",
        "label": "Class location"
      }
    },
    "class_target": {
      "type": "Select",
      "config": {
        "options": [
          "Learning Japanese",
          "Teaching Japanese"
        ],
        "default_value": "Learning Japanese",
        "label": "Class target"
      }
    },
    "class_learning_style": {
      "type": "Select",
      "config": {
        "options": [
          "Intensive Learning",
          "Regular Classes",
          "Drop-in Sessions"
        ],
        "label": "Class Learning Style"
      }
    },
    "class_popular": {
      "type": "Boolean",
      "config": {
        "default_value": false,
        "label": "Class Popular"
      }
    },
    "class_main_image": {
      "type": "Image",
      "config": {
        "constraint": {
          "width": 960,
          "height": 540
        },
        "thumbnails": [
          {
            "name": "Tablet",
            "width": 480,
            "height": 270
          },
          {
            "name": "thumbnail",
            "width": 240,
            "height": 135
          }
        ],
        "label": "Class Main Image"
      }
    },
    "class_duration": {
      "type": "Select",
      "config": {
        "options": [
          "6-week course",
          "8-week course",
          "Intensive weekend",
          "50-minute sessions",
          "Weekly drop-in sessions"
        ],
        "label": "Class Duration"
      }
    },
    "class_schedule": {
      "type": "Select",
      "config": {
        "options": [
          "Every Friday, 5pm - 6pm BST",
          "Every Friday, 6pm - 8pm BST",
          "Times on request",
          "Times vary based on package selected"
        ],
        "label": "Class Schedule"
      }
    },
    "class_dates": {
      "type": "Group",
      "config": {
        "fields": {
          "class_date": {
            "type": "Date",
            "config": {
              "label": "Class Date"
            }
          }
        },
        "label": "Class Dates"
      }
    },
    "class_summary": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph, preformatted, heading1, heading2, heading3, heading4, heading5, heading6, strong, em, hyperlink, image, embed, list-item, o-list-item, o-list-item",
        "label": "Class Summary"
      }
    },
    "class_discovery": {
      "type": "StructuredText",
      "config": {
        "multi": "list-item",
        "label": "Class Discovery"
      }
    },
    "class_curriculum": {
      "type": "StructuredText",
      "config": {
        "multi": "list-item",
        "label": "Class Curriculum"
      }
    },
    "class_details": {
      "type": "StructuredText",
      "config": {
        "multi": "list-item",
        "label": "Class Content"
      }
    },
    "class_bonus_content": {
      "type": "StructuredText",
      "config": {
        "multi": "list-item",
        "label": "Class Bonus Content"
      }
    },
    "class_price_options": {
      "type": "Group",
      "config": {
        "fields": {
          "price_option_title": {
            "type": "StructuredText",
            "config": {
              "single": "heading1, heading2, heading3, heading4, heading5, heading6",
              "label": "Price Option Title"
            }
          },
          "price_option_details": {
            "type": "StructuredText",
            "config": {
              "multi": "list-item",
              "label": "Price Option Details"
            }
          },
          "price": {
            "type": "Number",
            "config": {
              "label": "Price"
            }
          }
        },
        "label": "Class Price Options"
      }
    },
    "class_booking_title": {
      "type": "StructuredText",
      "config": {
        "single": "heading1, heading2, heading3, heading4, heading5, heading6",
        "label": "Class Booking Title"
      }
    },
    "class_booking_instructions": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph, preformatted, heading1, heading2, heading3, heading4, heading5, heading6, strong, em, hyperlink, image, embed, list-item, o-list-item, o-list-item",
        "label": "Class Booking Instructions"
      }
    },
    "class_booking_link": {
      "type": "Link",
      "config": {
        "allowTargetBlank": true,
        "label": "Class Booking Link"
      }
    },
    "class_images": {
      "type": "Group",
      "config": {
        "fields": {
          "class_image": {
            "type": "Image",
            "config": {
              "constraint": {
                "width": 960,
                "height": 540
              },
              "thumbnails": [
                {
                  "name": "Tablet",
                  "width": 480,
                  "height": 270
                },
                {
                  "name": "thumbnail",
                  "width": 240,
                  "height": 135
                }
              ],
              "label": "ClassImage"
            }
          }
        },
        "label": "Class Images"
      }
    },
    "class_faqs": {
      "type": "Group",
      "config": {
        "fields": {
          "class_faq_question": {
            "type": "StructuredText",
            "config": {
              "single": "heading3",
              "label": "Class FAQ Question"
            }
          },
          "class_faq_answer": {
            "type": "StructuredText",
            "config": {
              "multi": "paragraph, preformatted, heading1, heading2, heading3, heading4, heading5, heading6, strong, em, hyperlink, image, embed, list-item, o-list-item, o-list-item",
              "allowTargetBlank": true,
              "label": "Class FAQ Answer"
            }
          }
        },
        "label": "Class FAQs"
      }
    }
  }
}